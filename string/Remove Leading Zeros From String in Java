import java.util.*;
public class RemoveLeadZero{
    public static void main(String args[]){
        String str="000000045097012";
        String str2="";
        int index=0;
        for(int i=0;i<str.length();i++){
            if(str.charAt(i)!='0'){
                index=i;
                break;
            }
        }
        str2=str.substring(index,str.length());
        System.out.println(str2);
    }
}
